{
  "name": "gpsa/gerenciador-igrejas-api",
  "description": "Skeleton Application for Apigility",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "api",
    "apigility",
    "zf",
    "framework"
  ],
  "homepage": "http://apigility.org/",
  "support": {
    "email": "apigility-users@zend.com",
    "irc": "irc://irc.freenode.net/apigility",
    "source": "https://github.com/zfcampus/zf-apigility-skeleton",
    "issues": "https://github.com/zfcampus/zf-apigility-skeleton/issues"
  },
  "config": {
    "process-timeout": 5000
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.5.x-dev",
      "dev-develop": "1.6.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0",
    "zendframework/zend-component-installer": "^1.1.1 || ^2.1.1",
    "zfcampus/zf-apigility": "^1.4",
    "zfcampus/zf-apigility-documentation": "^1.3",
    "zfcampus/zf-development-mode": "^3.2",
    "zfcampus/zf-apigility-doctrine": "^2.1.0",
    "api-skeletons/zf-oauth2-doctrine": "3.1.0",
    "doctrine/doctrine-orm-module": "1.1.*",
    "doctrine/migrations": "^1.6",
    "api-skeletons/zf-doctrine-hydrator": "*",
    "api-skeletons/zf-oauth2-doctrine-console": "*",
    "zfr/zfr-cors": "1.*",
    "zfcampus/zf-doctrine-querybuilder": "^1.5",
    "beberlei/DoctrineExtensions": "^1.0",
    "phpoffice/phpspreadsheet": "1.8.0",
    "api-skeletons/zf-oauth2-doctrine-identity": "3.0.1",
    "zendframework/zend-permissions-rbac": "^3.0"
  },
  "require-dev": {
    "zendframework/zend-developer-tools": "^1.2.1",
    "zendframework/zend-test": "^2.6.1 || ^3.2",
    "zfcampus/zf-apigility-admin": "^1.6",
    "zfcampus/zf-asset-manager": "^1.2",
    "zfcampus/zf-composer-autoloading": "^1.1.1 || ^2.1",
    "zfcampus/zf-deploy": "^1.3"
  },
  "suggest": {
    "zfcampus/zf-apigility-doctrine": "zfcampus/zf-apigility-doctrine ^2.1 to create Doctrine-Connected REST services",
    "zfcampus/zf-http-cache": "zfcampus/zf-http-cache ^1.3 to add HTTP caching to your API",
    "zfr/zfr-cors": "zfr/zfr-cors ^1.2 to add CORS support to your API"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "module/Application/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApplicationTest\\": "module/Application/test/"
    },
    "files": [
      "src/Apigility/constants.php"
    ]
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "test": "phpunit"
  }
}
